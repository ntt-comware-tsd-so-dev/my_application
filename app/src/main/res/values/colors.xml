    <!--

    Application configuration colors

     Update these colors to change the look and feel of the app. The "app_theme_..."
     colors can be changed by themselves to quickly change the entire app, or
     individual colors can be changed below if desired.

    -->

    <color name="app_theme_accent">@color/light_blue_A400</color>
    <color name="app_theme_accent_light">@color/light_blue_A200</color>
    <color name="app_theme_accent_text">@android:color/white</color>
    <color name="app_theme_primary_light">@color/dark_orange_50</color>
    <color name="app_theme_primary_medium_light">@color/dark_orange_100</color>
    <color name="app_theme_primary">@color/dark_orange_200</color>
    <color name="app_theme_primary_medium_dark">@color/dark_orange_300</color>
    <color name="app_theme_primary_dark">@color/dark_orange_400</color>

