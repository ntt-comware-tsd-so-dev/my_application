    <color name="app_theme_window_background">@color/orange_grey_100</color>

    <!-- Colors of the navigation drawer background and text -->
    <color name="navigation_drawer">@color/app_theme_primary_dark</color>
    <color name="navigation_drawer_text">@color/app_theme_primary_light</color>

    <!-- Specific color assignments -->

    <color name="title_bar">@color/app_theme_primary_medium_dark</color>
    <color name="title_bar_text">@android:color/white</color>

    <!-- Colors for card views -->
    <color name="card_background">@color/grey_200</color>
    <color name="card_text">@android:color/black</color>
    <color name="card_detail_text">@color/grey_400</color>

    <!-- Color for text on a shared device -->
    <color name="card_shared_text">@color/app_theme_accent</color>

    <!-- Color for the text shown in an empty list -->
    <color name="empty_list_text">@color/app_theme_primary_medium_dark</color>

    <!-- Text color indicating "disabled" -->
    <color name="disabled_text">@color/grey_600</color>
    <color name="enabled_text">@android:color/white</color>

    <!-- Default button background gradients -->
    <color name="button_start">@color/app_theme_primary_light</color>
    <color name="button_end">@color/app_theme_primary</color>
    <color name="button_selected_start">@color/app_theme_primary_medium_light</color>
    <color name="button_selected_end">@color/app_theme_primary_medium_dark</color>
    <color name="button_disabled_start">@color/grey_500</color>
    <color name="button_disabled_end">@color/grey_600</color>

    <!-- Colors for normal or selected list items -->
    <color name="list_bg_normal">@android:color/white</color>
    <color name="list_bg_selected">@color/app_theme_primary_medium_light</color>

    <!-- Color for the round "+" button -->
    <color name="add_button_bg">@color/app_theme_accent</color>
    <color name="add_button_fg">@color/app_theme_accent_text</color>

    <!-- Colors for detail views -->
    <color name="device_details_background">@color/app_theme_window_background</color>
    <color name="device_details_bottom_bar_background">@color/app_theme_primary_dark</color>

    <!-- Colors for the schedule page -->
    <color name="schedule_background">@color/app_theme_window_background</color>
    <color name="schedule_type_background">@color/app_theme_primary_light</color>
    <color name="schedule_weekday_button_selected">@color/app_theme_primary_medium_dark</color>
    <color name="schedule_weekday_button_deselected">@color/grey_600</color>

    <!-- Colors for the WiFi Setup page -->
    <color name="wifi_setup_background">@color/app_theme_window_background</color>

    <!-- Colors for sign up / sign in pages -->
    <color name="sign_up_background">@color/app_theme_window_background</color>

    <!-- Colors for destructive operations, such as deleting, unregistering, etc. -->
    <color name="destructive_bg">@color/red_900</color>
    <color name="destructive_fg">#FFFFFF</color>

    <!-- Colors of the bars under the blue / green bulbs -->
    <color name="blue_bulb_bg">#88003388</color>
    <color name="green_bulb_bg">#88008800</color>

    <!-- Color of the link text on the login screen -->
    <color name="link">#00688B</color>

    <!-- Color of device power on/off -->
    <color name="device_on_bg">@color/green_500</color>
    <color name="device_off_bg">@color/grey_500</color>
    <color name="device_pending_bg">@color/yellow_500</color>
    <color name="device_offline_bg">@color/grey_700</color>
    <color name="device_onoff_fg">@android:color/white</color>

</resources>